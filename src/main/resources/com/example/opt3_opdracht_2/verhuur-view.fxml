<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="570.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.opt3_opdracht_2.VerhuurController">
   <children>
      <TextArea fx:id="DetailsArea" layoutX="16.0" layoutY="13.0" prefHeight="228.0" prefWidth="277.0" />
      <TextArea fx:id="VerhuurArea" layoutX="16.0" layoutY="285.0" prefHeight="228.0" prefWidth="277.0" />
      <TextField fx:id="AchternaamField" layoutX="363.0" layoutY="156.0" promptText="Achternaam" />
      <TextField fx:id="DagenField" layoutX="363.0" layoutY="241.0" promptText="Aantal dagen" />
      <Button layoutX="341.0" layoutY="325.0" mnemonicParsing="false" onAction="#OnVerhuurBtnClick" text="Verhuur" />
      <Button layoutX="464.0" layoutY="498.0" mnemonicParsing="false" onAction="#OnTerugBtnClick" text="Naar Overzicht" />
      <TextField fx:id="NaamField" layoutX="363.0" layoutY="75.0" promptText="Voornaam" />
      <ToggleButton fx:id="VerzekerBtn" layoutX="439.0" layoutY="325.0" mnemonicParsing="false" text="Verzekeren?" />
      <Button fx:id="RetourBtn" layoutX="439.0" layoutY="375.0" mnemonicParsing="false" onAction="#OnRetourneerBtnClick" text="Retourneren" />
      <Label fx:id="Medewerkerslabel" layoutX="526.0" layoutY="18.0" text="Label" />
      <ImageView fx:id="image" fitHeight="27.0" fitWidth="26.0" layoutX="490.0" layoutY="13.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@avatar.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
