         <image>
            <Image url="@avatar.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="FactoryView" layoutX="47.0" layoutY="89.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="Soort" prefWidth="199.0" text="Product Soorten" />
        </columns>
      </TableView>
      <Button fx:id="createProductBtn" layoutX="300.0" layoutY="185.0" mnemonicParsing="false" onAction="#OnCreateBtnClick" text="createProduct" />
      <Button fx:id="addProductBtn" layoutX="305.0" layoutY="259.0" mnemonicParsing="false" onAction="#OnAddBtnClick" text="addProduct" />
